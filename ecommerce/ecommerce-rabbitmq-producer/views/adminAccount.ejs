                            <div aria-hidden="true" aria-labelledby="myModalLabel" role="dialog" tabindex="-1"
                                id="myModalUser" class="modal fade">
                                <div class="modal-dialog">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <h4 class="title"> Edit User</h4>
                                            <button aria-hidden="true" data-dismiss="modal" class="close"
                                                type="button">×</button>

                                        </div>
                                        <div class="modal-body">
                                            <div class="form-floating">
                                                <label for="Name">Name</label>
                                                <input type="text" class="form-control" id="name" disabled>
                                            </div>
                                            <div class="form-floating">
                                                <label for="Phone">Phone</label>
                                                <input type="tel" class="form-control" id="phone" disabled>
                                            </div>
                                            <div class="form-floating">
                                                <label for="email">Email Address</label>
                                                <input type="email" class="form-control" id="email" disabled>
                                            </div>
                                            <div class="form-floating">
                                                <label for="Address">Address</label>
                                                <input type="text" class="form-control" id="address" disabled>
                                            </div>
                                            <div class="form-group">
                                                <label for="sel1">Action:</label>
                                                <select class="form-control" id="sel1">
                                                    <option value="" selected></option>
                                                    <option value="verify">Require user to verify account</option>
                                                    <option value="lock">Lock this account</option>
                                                    <option value="unlock">Unlock this account</option>
                                                </select>
                                            </div>

                                            <div class="d-grid mb-4 mt-4" style="border-radius:5px ;">
                                                <button onclick="submitEditUser()" class="btn4 btn-primary">Submit</button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>


                            <div aria-hidden="true" aria-labelledby="myModalLabel" role="dialog" tabindex="-1"
                                id="myModal2" class="modal fade">
                                <div class="modal-dialog">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <h4 class="title"> Add new user</h4>
                                            <button aria-hidden="true" data-dismiss="modal" class="close"
                                                type="button">×</button>

                                        </div>
                                        <div class="message">
                                            <div class="alert alert-danger" role="alert" id="alert"
                                                style="display:none">
                                            </div>
                                        </div>
                                        <div class="modal-body">
                                            <div class="row">
                                                <div class="form-floating col-sm-6">
                                                    <label for="firstName">First Name</label>
                                                    <input type="text" class="form-control" id="firstNameAdd"
                                                        placeholder="First Name" name="firstName">
                                                </div>
                                                <div class="form-floating col-sm-6">
                                                    <label for="lastName">Last Name </label>
                                                    <input type="text" class="form-control" id="lastNameAdd"
                                                        placeholder="Last Name" name="lastName">
                                                </div>
                                            </div>

                                            <div class="form-floating">
                                                <label for="email">Email</label>
                                                <input type="text" class="form-control" name="email" placeholder="Email"
                                                    id="emailAdd">
                                            </div>
                                            <div class="form-floating">
                                                <label for="phone">Phone</label>
                                                <input type="number" class="form-control" placeholder="Phone number"
                                                    name="phone" id="phoneAdd">
                                            </div>
                                            <div class="form-floating">
                                                <label for="email">password</label>
                                                <input type="password" class="form-control" placeholder="Password"
                                                    name="password" id="passwordAdd">
                                            </div>
                                            <div class="form-group">
                                                <label for="sel1Add">Role:</label>
                                                <select class="form-control" id="sel1Add">
                                                    <option value="1">User</option>
                                                    <option value="2">Warehouse staff</option>
                                                    <option value="3">Store management</option>
                                                    <option value="4">Admin</option>
                                                </select>
                                            </div>
                                            <div class="d-grid mb-4 mt-4" style="border-radius:5px ;">
                                                <button onclick="submitNewAcc()"
                                                    class="btn4 btn-primary">Submit</button>
                                            </div>


                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>


                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-lg-12 col-md-12">
