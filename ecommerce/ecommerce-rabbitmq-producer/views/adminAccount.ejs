    <div id="content" style="margin-top:110px ;">
        <div class="main-content">

            <div class="row">
                <div class="container-fluid">
                    <div class="row">
                        <button class="btn btn-secondary ml-auto btn3 mb-4" href="#myModal2" data-toggle="modal"
                            style="margin-right:60px;">Create new account</button>
                    </div>
                </div>
            </div>


            <div class="row ">
                <div class="col-lg-12 col-md-12">
                    <div class="card" style="min-height: 485px">
                        <div class="card-header card-header-text">
                            <h4 class="card-title" style="font-size: 30px;">User</h4>
                        </div>

                        <div class="card-content table-responsive">
                            <table class="table table-hover">
                                <thead class="text-primary">
                                    <tr>
                                        <th>Email</th>
                                        <th>Name</th>
                                        <th>Phone</th>
                                        <th>Addres</th>
                                        <th>Role</th>
                                        <th>Status</th>

                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <%users.forEach((user)=>{%>
                                        <tr>
                                            <td>
                                                <%=user.email%>
                                            </td>
                                            <td>
                                                <%=user.name%>
                                            </td>
                                            <td>
                                                <%=user.phone%>
                                            </td>
                                            <td>
                                                <%=user.address%>
                                            </td>
                                            <td>
                                                <%=user.role%>
                                            </td>
                                            <td>
                                                <%=user.status%>
                                            </td>

                                            <td> <button class="btn btn1" href="#myModalUser" data-toggle="modal"
                                                    onclick="filInfor('<%=user.email%>','<%=user.phone%>','<%=user.address%>','<%=user.name%>')">Edit</button>
                                                <button class="btn btn2" onclick = "deleteAcc('<%=user.email%>')">Delete</button>
                                            </td>
                                        </tr>
                                        <%})%>

                                </tbody>

                            </table>
