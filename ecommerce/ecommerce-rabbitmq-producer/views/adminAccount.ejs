                                            <label for="Name">Name</label>
                                            <input type="text" class="form-control" id="nameAdmin" >
                                        </div>
                                        <div class="form-floating">
                                            <label for="Phone">Phone</label>
                                            <input type="tel" class="form-control" id="phoneAdmin" >
                                        </div>
                                  
                                    
                                        <div class="form-group">
                                            <label for="sel1">Action:</label>
                                            <select class="form-control" id="sel1Admin">
                                                <option value="" selected></option>
                                                <option value="lock">Lock this account</option>
                                                <option value="unlock">Unlock this account</option>
                                            </select>
                                        </div>

                                        <div class="d-grid mb-4 mt-4" style="border-radius:5px ;">
                                            <button onclick="submitEditAdmin()" class="btn4 btn-primary">Submit</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>


