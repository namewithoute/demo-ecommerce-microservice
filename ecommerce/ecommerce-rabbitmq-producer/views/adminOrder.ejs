                            <h4 class="card-title" style="font-size: 30px; text-align: center;">Order</h4>

                        </div>
                        <div class="card-content table-responsive">
                            <table class="table table-hover">
                                <thead class="text-primary">
                                    <tr>
                                        <th>STT</th>
                                        <th>ID</th>
                                        <th>Customer</th>
                                        <th>Total</th>
                                        <th>Create at</th>
                                        <th>Payment status</th>
                                        <th>Delivery status</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <%const formatter = new Intl.NumberFormat('en-US', {
                                    style: 'currency',
                                    currency: 'VND',
                                  
                                  });%>
                                <tbody>
                                    <%orders.forEach((order,index)=>{%>
                                        <tr>
                                            <td><%=index+1%></td>
                                            <td><%=order.orderID%></td>
                                            <td><%=order.name%></td>
                                            <td><%=formatter.format(order.total)%></td>
                                            <td><%=order.createAt%></td>
                                            <td><%=order.isPaid ?'Paid' :'Unpaid'%></td>
                                            <td><%=order.status%></td>
                                            <td>
                                                <a class="btn btn1" href="/admin/orders/<%=order.orderID%>" >View</a>
                                            </td>
                                        </tr>
                                    <%})%>
                                </tbody>
                            </table>
